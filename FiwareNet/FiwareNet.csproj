<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>

    <Version>1.0.0</Version>
    <Authors>Fabian Spitzer</Authors>
    <Company>FH OÖ F&amp;E</Company>
    <Description>A C# client library to connect to an Orion Context Broker using the FIWARE protocol.</Description>
    <PackageProjectUrl>https://sar.fh-ooe.at/</PackageProjectUrl>
    <Copyright>Copyright © 2021-2024 Fabian Spitzer</Copyright>

    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MQTTnet" Version="4.3.3.952" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
    <PackageReference Include="RestSharp" Version="110.2.0" />
    <PackageReference Include="RestSharp.Serializers.NewtonsoftJson" Version="110.2.0" />
  </ItemGroup>

</Project>